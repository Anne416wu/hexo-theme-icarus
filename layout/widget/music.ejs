
 <% function get_music() {
     if(get_config('neteaseMusic.enable')===true){
        return get_config('neteaseMusic');
     }
    else{
        return '';
    }
} %>

<iframe class="card widget" height=400 width=100% src="//music.163.com/outchain/player?type=0&id=<%= get_music() %>&auto=0&height=430"></iframe>
